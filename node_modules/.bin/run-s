#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/npm-run-all/4.1.5/node_modules/npm-run-all/bin/run-s/index.js/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/npm-run-all/4.1.5/node_modules/npm-run-all/bin/run-s/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/npm-run-all/4.1.5/node_modules/npm-run-all/bin/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/npm-run-all/4.1.5/node_modules/npm-run-all/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/npm-run-all/4.1.5/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/npm-run-all/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/node_modules:/mnt/c/Users/dhruv/Documents/node_modules:/mnt/c/Users/dhruv/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/npm-run-all/bin/run-s/index.js/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/npm-run-all/bin/run-s/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/npm-run-all/bin/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/npm-run-all/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../npm-run-all/bin/run-s/index.js" "$@"
else 
  exec node  "$basedir/../npm-run-all/bin/run-s/index.js" "$@"
fi
