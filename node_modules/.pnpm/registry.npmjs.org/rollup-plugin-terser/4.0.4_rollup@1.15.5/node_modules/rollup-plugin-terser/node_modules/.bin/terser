#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/terser/3.17.0/node_modules/terser/bin/uglifyjs/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/terser/3.17.0/node_modules/terser/bin/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/terser/3.17.0/node_modules/terser/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/terser/3.17.0/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/terser/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/node_modules:/mnt/c/Users/dhruv/Documents/node_modules:/mnt/c/Users/dhruv/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../terser/3.17.0/node_modules/terser/bin/uglifyjs" "$@"
else 
  exec node  "$basedir/../../../../../../terser/3.17.0/node_modules/terser/bin/uglifyjs" "$@"
fi
