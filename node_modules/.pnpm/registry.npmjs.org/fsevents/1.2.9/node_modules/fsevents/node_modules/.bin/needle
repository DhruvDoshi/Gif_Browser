#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/needle/bin/needle/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/needle/bin/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules/needle/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules/fsevents/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/fsevents/1.2.9/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/fsevents/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/node_modules:/mnt/c/Users/dhruv/Documents/node_modules:/mnt/c/Users/dhruv/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../needle/bin/needle" "$@"
else 
  exec node  "$basedir/../needle/bin/needle" "$@"
fi
