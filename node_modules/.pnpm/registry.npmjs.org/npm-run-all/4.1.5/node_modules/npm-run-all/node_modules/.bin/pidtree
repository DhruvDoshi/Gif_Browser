#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/pidtree/0.3.0/node_modules/pidtree/bin/pidtree.js/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/pidtree/0.3.0/node_modules/pidtree/bin/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/pidtree/0.3.0/node_modules/pidtree/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/pidtree/0.3.0/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/pidtree/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules/.pnpm/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/New folder/node_modules:/mnt/c/Users/dhruv/Documents/GitHub/node_modules:/mnt/c/Users/dhruv/Documents/node_modules:/mnt/c/Users/dhruv/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../pidtree/0.3.0/node_modules/pidtree/bin/pidtree.js" "$@"
else 
  exec node  "$basedir/../../../../../../pidtree/0.3.0/node_modules/pidtree/bin/pidtree.js" "$@"
fi
